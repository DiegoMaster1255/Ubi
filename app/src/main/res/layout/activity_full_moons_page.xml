<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FullMoonsPage">

    <Button
            android:text="Odejmij"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/subButton" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.18"/>
    <Button
            android:text="Wyszukaj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/confirmButton" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.878" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.18"/>
    <Button
            android:text="Wróć"
            android:layout_width="89dp"
            android:layout_height="wrap_content"
            android:id="@+id/backButton" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.881" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.064"/>
    <Button
            android:text="Dodaj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/addButton" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.049" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.18"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName|text"
            android:text="2019"
            android:ems="10"
            android:id="@+id/yearInput"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.196"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.068"/>
    <ScrollView
            android:layout_width="328dp"
            android:layout_height="294dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.529"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.697">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.138" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.297" android:text="1"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView2"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.416"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.283"
                    android:text="2"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView3"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.898"
                    android:text="3"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView4"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.841"
                    android:text="4"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView5"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.78"
                    android:text="5"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView6"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.712"
                    android:text="6"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView7"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.654"
                    android:text="7"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView8"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.588"
                    android:text="8"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView9"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.529"
                    android:text="9"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView10"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.467"
                    android:text="10"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView11"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.099"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.404"
                    android:text="11"/>
            <TextView
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:id="@+id/textView12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.1" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.346" android:text="12"/>
        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>